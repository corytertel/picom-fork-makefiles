prefix=/usr/X11R7
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include
datarootdir=${prefix}/share
icondir=${datarootdir}/icons

Name: Xcursor
Description: X Cursor Library
Version: 1.2.0
Requires: xproto
Requires.private: x11 xrender xfixes
Cflags: -I${includedir}
Libs: -Wl,-rpath,${libdir} -L${libdir} -lXcursor
