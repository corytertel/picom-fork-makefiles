prefix=/usr/X11R7
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Xfont
Description: X font Library
Version: 1.5.4
Requires: xproto fontsproto
Requires.private: fontenc freetype2
Cflags: -I${includedir}
Libs: -Wl,-rpath,${libdir} -L${libdir} -lXfont
Libs.private: -lz -lm
